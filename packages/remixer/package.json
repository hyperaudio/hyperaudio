{
  "name": "@hyperaudio/remixer",
  "version": "0.1.0",
  "description": "Made with create-react-library",
  "license": "AGPL-3.0-only",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs --external=react,react-dom,react-is --jsxFragment React.Fragment --jsx React.createElement",
    "start": "microbundle-crl watch --no-compress --format modern,cjs --external=react,react-dom,react-is --jsxFragment React.Fragment --jsx React.createElement",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom"
  },
  "peerDependencies": {
    "@emotion/react": "11.7.1",
    "@emotion/styled": "11.6.0",
    "@mui/icons-material": "5.4.1",
    "@mui/material": "5.4.1",
    "react": ">=17.0.2",
    "react-dom": ">=17.0.2"
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "cross-env": "7.0.3",
    "eslint": "8.8.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "16.0.3",
    "eslint-config-standard-react": "11.0.1",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-standard": "4.1.0",
    "gh-pages": "3.2.3",
    "microbundle-crl": "0.13.11",
    "npm-run-all": "4.1.5",
    "prettier": "2.5.1"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "array-move": "4.0.0",
    "create-silent-audio": "0.1.1",
    "lodash": "4.17.21",
    "react-beautiful-dnd": "^13.1.0",
    "react-draggable": "4.4.4",
    "react-player": "2.9.0",
    "smpte-timecode": "1.2.3"
  }
}
